@model HomePageViewModel

<div class="col-md-8 offset-md-2">
    @foreach (var image in Model.Images)
    {
        <div class="bg-light p-4 rounded shadow mb-3" style="margin-bottom: 20px; text-align: center;">
            <a href="/home/viewimage?id=@image.Id">
                <h3>@image.Title</h3>
                <h5>Uploaded at: 04/27/2023 02:10 AM</h5>
                <img style="width: 400px;" src=@image.Src />
            </a>
        </div>
    }
</div>